{
  "name": "botson-frontend",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently --kill-others -p [{name}] -n \"API,NG2\" -c \"bgGreen.bold,bgRed.bold\" \"./node_modules/.bin/nodemon -e js index.js\" \"ng serve --progress=false\"",
    "stage": "concurrently --kill-others -p [{name}] -n \"NG2,API\" -c \"bgRed.bold,bgGreen.bold\" \"ng build --prod --watch=true\" \"NODE_ENV=production ./node_modules/.bin/nodemon -e js index.js\"",
    "prod": "NODE_ENV=production node index.js",
    "build": "ng build --prod --sm=false --aot=true",
    "start": "npm run dev",
    "test": "ng test",
    "lint": "tslint \"src/**/*.ts\"",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.broadsoft.com/BroadSoftLabs/botson-frontend.git"
  },
  "homepage": "https://git.broadsoft.com/BroadSoftLabs/botson-frontend#readme",
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "co": "^4.6.0",
    "core-js": "^2.4.1",
    "koa": "^1.1.2",
    "koa-router": "^7.1.1",
    "koa-static": "^1.5.2",
    "moment": "^2.18.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
